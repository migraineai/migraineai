        "title": "The Basics",
        "questions": [
          {
            "id": "q1_frequency",
            "question_text": "On average, how many days per month do you have a headache or migraine?",
            "voice_prompt": "On average, how many days per month do you have a headache or migraine?",
            "ui_type": "single_select",
            "options": [
              { "label": "0–4 days (Low frequency)", "value": "low_freq" },
              { "label": "5–9 days", "value": "med_freq" },
              { "label": "10–14 days", "value": "high_freq" },
              { "label": "15 or more days (Chronic)", "value": "chronic" }
            ]
          },
          {
            "id": "q2_duration",
            "question_text": "Without medication, how long do your attacks typically last?",
            "voice_prompt": "Without medication, how long do your attacks usually last?",
            "ui_type": "single_select",
            "options": [
              { "label": "Less than 4 hours", "value": "under_4h" },
              { "label": "4 to 72 hours", "value": "4_to_72h" },
              { "label": "More than 3 days", "value": "over_72h" }
            ]
          },
